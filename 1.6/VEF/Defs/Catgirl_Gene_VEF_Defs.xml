<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <GeneDef>
    <defName>BBLK_CatMeat</defName>
    <label>cat flesh</label>
    <description>Carriers of this gene have cat flesh.</description>
    <iconPath>Things/Item/Resource/MeatFoodRaw/Meat_Small/Meat_Small_b</iconPath>
    <biostatCpx>2</biostatCpx>
    <biostatMet>0</biostatMet>
    <selectionWeight>0</selectionWeight>
    <displayOrderInCategory>111</displayOrderInCategory>
    <displayCategory>Miscellaneous</displayCategory>
    <exclusionTags>
      <li>Flesh</li>
      <li>Meat</li>
      <li>AG_Meat</li>
    </exclusionTags>
    <modExtensions>
      <li Class="VEF.Genes.GeneExtension">
        <customMeatThingDef>Meat_Cat</customMeatThingDef>
      </li>
    </modExtensions>
  </GeneDef>

  <GeneDef>
    <defName>BBLK_PantherMeat</defName>
